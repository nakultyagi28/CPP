option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "CPP.settings"
    "PYTHONPATH": "/opt/python/current/app/CPP:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: CPP/CPP/wsgi.py
    NumProcesses:5
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
